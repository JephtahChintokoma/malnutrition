// start the session
session_start();

// Check if the user is not logged in, then redirect the user to login page
if (!isset($_SESSION[&quot;userid&quot;]) || $_SESSION[&quot;userid&quot;] !== true) {
    header(&quot;location: login.php&quot;);
    exit;
}
